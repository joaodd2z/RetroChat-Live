# Configuração para produção no Vercel
REACT_APP_FIREBASE_API_KEY=demo-key
REACT_APP_FIREBASE_AUTH_DOMAIN=demo.firebaseapp.com
REACT_APP_FIREBASE_PROJECT_ID=demo-project
REACT_APP_FIREBASE_STORAGE_BUCKET=demo.appspot.com
REACT_APP_FIREBASE_MESSAGING_SENDER_ID=123456789
REACT_APP_FIREBASE_APP_ID=1:123456789:web:demo
REACT_APP_FIREBASE_DATABASE_URL=https://demo.firebaseio.com/

# Configuração do servidor
REACT_APP_SERVER_URL=https://retro-chat-live.vercel.app
REACT_APP_SOCKET_URL=https://retro-chat-live.vercel.app

# Modo de produção
NODE_ENV=production
GENERATE_SOURCEMAP=false

# Configurações de build
BUILD_PATH=build
PUBLIC_URL=/

# Desabilitar warnings em produção
CI=false
ESLINT_NO_DEV_ERRORS=true
TSC_COMPILE_ON_ERROR=true